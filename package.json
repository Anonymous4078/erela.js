{
  "name": "erela.js",
  "version": "0.0.0",
  "description": "An easy to use Discord audio client for Node.JS supporting Lavalink and FFMPEG.",
  "main": "dist/index.js",
  "repository": "https://github.com/Erelajs/erela.js",
  "author": "MenuDocs (https://github.com/MenuDocs)",
  "license": "Apache-2.0",
  "types": "./typings/index.d.ts",
  "scripts": {
    "build": "tsc",
    "lint": "eslint --ext .ts ./src",
    "fix": "eslint --ext .ts ./src --fix",
    "types": "dts-bundle --removeSource --name erela.js --main ./dist --out ../typings/index.d.ts --baseDir ./dist"
  },
  "dependencies": {
    "@discordjs/collection": "^0.1.6"
  },
  "devDependencies": {
    "@types/node": "^15.12.2",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "eslint": "^7.29.0",
    "eslint-config-tesseract": "MenuDocs/eslint-config-tesseract",
    "typescript": "^4.3.5",
    "dts-bundle": "^0.7.3"
  },
  "eslintConfig": {
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "rules": {
      "valid-jsdoc": "off"
    },
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:@typescript-eslint/eslint-recommended",
      "tesseract"
    ]
  }
}
